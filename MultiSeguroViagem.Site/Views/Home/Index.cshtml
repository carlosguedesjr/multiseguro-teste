    <script async>
        //============================================================================================= SCRIPTS QUE PODEM SER ASYNC
        $('.carousel-client').bxSlider({
            auto: true,
            minSlides: 1,
            maxSlides: 5,
            controls: true,
            pager: false,
            nextText: '<i class="fa fa-angle-right"></i>',
            prevText: '<i class="fa fa-angle-left"></i>'
        });

        var model = @Html.Raw(Json.Encode(Model));
        if (model) {
            $('#btnCotar').addClass('disabled').removeClass('btn-primary');
            $('#in-col-md-offset-2-home').removeClass('hidden').addClass('slideDown');
        }

        $('#Email, #EmailBanner').blur(function(evt) {
            setSession('email', evt.target.value);
            setSession('contador', 0);
        });

        $('.carousel-testimonials').bxSlider({
            auto: true,
            minSlides: 1,
            maxSlides: 3,
            moveSlides: 1,
            controls: true,
            slideWidth: 390,
            pager: false,
            nextText: '<i class="fa fa-angle-right"></i>',
            prevText: '<i class="fa fa-angle-left"></i>'
        });

        $('.carousel-testimonials-2').bxSlider({
            auto: true,
            minSlides: 1,
            maxSlides: 1,
            slideMargin: 25,
            moveSlides: 1,
            controls: false,
            pager: true,
            nextText: '<i class="fa fa-angle-right"></i>',
            prevText: '<i class="fa fa-angle-left"></i>'
        });


        $('#Origem').val(getLocalStorage('utmSource') || 'Indefinido');

        if (!isMobile(navigator.userAgent)) {
            $('#image').css('background-image', 'url(@caminhoImagemBackground)');
        }
    </script>

    <script>
        //============================================================================================= SCRIPTS QUE NÃO PODEM SER ASYNC
        setaUtmSource();

        setaCupomAfiliado();

        if (window.sessionStorage.getItem('cpaff')) {
            $('#Cpaff').val(window.sessionStorage.getItem('cpaff'));
        }
        else {
            $('.promo-linha').removeClass('hidden');
        }

        $("#Destino option:first").hide();
        $("#Destino").selectpicker('refresh');

        datepickerSetup();

        $(function () {
            datepickerSetaMinMaxDate("#DataIda", "#DataVolta");
            datepickerSetaMinMaxDate("#DataIdaBanner", "#DataVoltaBanner");
        });

        $('#btnCotar').click(function () {
            if (!validaForm('#frmCotacao')) return false;

            $('#btnCotar').addClass('disabled').removeClass('btn-primary');
            $('#in-col-md-offset-2-home').removeClass('hidden').addClass('slideDown');
        });
        var cont = 0;
        $('#btnResultado').click(function (event) {

            event.preventDefault();
            if (cont == 0) {
                if (validaForm('#frmCotacao')) {
                    var tokenProd = "1|QwRssH4OnhBrS0tc5MbgrY6Thx3DS36Z4PZjyth8";
                    var dataIntencao = {
                        'str_nome': $('#Nome').val(),
                        'str_telefone': $('#Telefone').val(),
                        'str_email': $('#Email').val(),
                        'dat_ida': $('#DataIda').val(),
                        'dat_volta': $('#DataVolta').val(),
                        'str_destino': $('#Destino :selected:not([value=""])').text(),
                        'str_origem': window.location.href
                    }

                    $.ajax({
                        method: "POST",
                        url: "https://api2.multiseguroviagem.com.br/api/intencoes/salva",
                        cache: false,
                        sync: false,
                        type: 'post',
                        data: JSON.stringify(dataIntencao),
                        contentType: 'application/json',
                        dataType: 'json',
                        headers: {
                            "Authorization": "Bearer " + tokenProd,
                            'Access-Control-Allow-Origin': 'https://api2.multiseguroviagem.com.br',
                            "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept, Authorization"
                        },
                        success: function (result) {

                            document.getElementById('frmCotacao').submit();
                        },
                        error: function (result) {

                            document.getElementById('frmCotacao').submit();
                        }
                    });


                    cont++;
                }

            }
            if (!validaForm('#frmCotacao')) return false;

        });

        $('form').one('submit',
            function () {

                conversaoRDStation("Formulário Home");
            });

        $(function () {
            enviaTagOneSignal(window.OneSignal, { "pgIndex": "true" });
        });
    </script>


                                        <div class="card-content promo-home" operadora="Travel Ace">
                                            <span class="ribbon4" operadora="Travel Ace">@banner.Tag.ToUpper()</span>
                                        </div>
                                    }

                                    var filtroOperadora = banner.FiltroOperadora != null ? banner.FiltroOperadora : "";
                                    var filtroDestino = banner.FiltroDestino != null ? banner.FiltroDestino : "";

                                    <input type="hidden" class="filtroOperadora-js" value="@filtroOperadora" />
                                    <input type="hidden" class="filtroDestino-js" value="@filtroDestino" />
                                }

                                <input type="hidden" class="tituloModal-js" value="@banner.TituloModal" />
